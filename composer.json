{
    "name": "nexmo/jwt",
    "description": "A standalone package for creating JWTs for Nexmo/Vonage APIs",
    "type": "library",
    "require": {
        "lcobucci/jwt": "^3.3",
        "ramsey/uuid": "^3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpstan/phpstan": "^0.12.25",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Tankersley",
            "email": "chris@ctankersley.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nexmo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexmo\\": "test/",
            "NexmoTest\\": "test/"
        }
    }
}
