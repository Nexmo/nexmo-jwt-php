{
    "name": "vonage/jwt",
    "description": "A standalone package for creating JWTs for Vonage APIs",
    "type": "library",
    "require": {
        "php": "^7.2|^8.0",
        "lcobucci/jwt": "^3.4|4.0.0",
        "ramsey/uuid": "^3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.4",
        "phpstan/phpstan": "^0.12",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "license": "Apache 2.0",
    "authors": [
        {
            "name": "Chris Tankersley",
            "email": "chris@ctankersley.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vonage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vonage\\": "test/",
            "VonageTest\\": "test/"
        }
    }
}
